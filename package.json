{
  "name": "sss-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter=frontend dev",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "format": "pnpm --recursive format",
    "db:generate": "pnpm --filter=database generate",
    "db:migrate": "pnpm --filter=database migrate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.12",
    "dotenv": "^16.5.0",
    "vitest": "^3.1.3"
  }
}
